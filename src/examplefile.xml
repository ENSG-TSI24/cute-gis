<GDAL_WMS>
    <Service name="WMS">
        <Version>1.3.0</Version>
        <ServerUrl>https://wxs.ign.fr/geoportail/wms</ServerUrl>
        <Layers>GEOGRAPHICALGRIDSYSTEMS.MAPS</Layers>
        <CRS>EPSG:4326</CRS>
        <BBox>4.776,45.641,5.045,45.841</BBox> <!-- BBox autour de Lyon -->
        <Format>image/jpeg</Format>
        <Transparent>false</Transparent>
    </Service>
    <DataWindow>
        <UpperLeftX>4.776</UpperLeftX>
        <UpperLeftY>45.841</UpperLeftY>
        <LowerRightX>5.045</LowerRightX>
        <LowerRightY>45.641</LowerRightY>
        <SizeX>256</SizeX>
        <SizeY>256</SizeY>
    </DataWindow>
    <BandsCount>3</BandsCount>
    <Cache />
</GDAL_WMS>